{
  "name": "notes",
  "version": "0.2.0",
  "description": "Web app that give the ability to note draggable notes to topics on boards",
  "main": "index.js",
  "scripts": {
    "flow:check": "flow check",
    "test": "jest",
    "test:watch": "jest --watch",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "lint": "eslint -c .eslintrc-build.js ./src > lint.txt",
    "css:min": "gulp",
    "css:sass": "gulp sass",
    "css:all": "npm run css:sass && npm run css:min",
    "build:dev:dll": "webpack --colors --progress --config webpack.dev.dll.js",
    "build:dev:dll-jsx": "webpack --colors --progress --config webpack.dev.dll-jsx.js",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "build:all": "npm run lint && npm run css:min && npm run build:dll-jsx"
  },
  "author": "Yevhenii Molodyi",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "react": "^17.0.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "del": "^5.1.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "flow-bin": "^0.136.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^25.1.0",
    "jspm": "^0.16.55",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "jspm": {
    "dependencies": {
      "@babel/runtime": "npm:@babel/runtime@^7.12.1",
      "react": "npm:react@16.14.0",
      "react-beautiful-dnd": "npm:react-beautiful-dnd@^11.0.5",
      "react-dom": "npm:react-dom@16.14.0",
      "react-redux": "npm:react-redux@^7.2.1",
      "react-router-dom": "npm:react-router-dom@^5.2.0",
      "redux": "npm:redux@^4.0.5"
    },
    "devDependencies": {}
  },
  "jest": {
    "roots": [
      "<rootDir>/js/utils/",
      "<rootDir>/js/flux/"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/js/utils/",
      "<rootDir>/js/flux/",
      "<rootDir>/js/component/"
    ]
  }
}
