{
  "name": "notes",
  "version": "0.1.0",
  "description": "Web app that give the ability to note draggable notes on boards",
  "main": "index.js",
  "scripts": {
    "flow:check": "flow check",
    "test": "jest",
    "test:watch": "jest --watch",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "lint": "eslint -c .eslintrc-build.json ./src > lint.txt",
    "css:min": "gulp",
    "css:sass": "gulp sass",
    "css:all": "npm run css:sass && npm run css:min",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.config-dll-jsx.js",
    "build:all": "npm run lint && npm run css:min && npm run build:dll-jsx"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "dependencies": {
    "react": "^16.4.2",
    "react-beautiful-dnd": "^9.0.0",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.2.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.80.0",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.10.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^4.3.2",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^8.1.1",
    "gulp-sass": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-processing-plugin": "^0.1.2",
    "jest": "^23.5.0",
    "jspm": "^0.16.53",
    "webpack": "^3.12.0"
  },
  "jspm": {
    "dependencies": {
      "react": "npm:react@^16.4.2",
      "react-beautiful-dnd": "npm:react-beautiful-dnd@^9.0.0",
      "react-dom": "npm:react-dom@^16.4.2",
      "react-redux": "npm:react-redux@^5.0.7",
      "react-router-dom": "npm:react-router-dom@^4.3.1",
      "redux": "npm:redux@^4.0.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/js/utils/",
      "<rootDir>/js/flux/"
    ]
  }
}
